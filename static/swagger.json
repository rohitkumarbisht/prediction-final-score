    "/prediction-eng/": {
      "post": {
        "tags": [
          "Prediction"
        ],
        "summary": "Predict on the trained model and update the result in the database for multiple record",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "features": {
                    "type": "string"
                  },
                  "ContentReads": {
                    "type": "integer"
                  },
                  "No_of_Logins": {
                    "type": "integer"
                  },
                  "Assignment2_delay": {
                    "type": "integer"
                  },
                  "Assignment3_delay": {
                    "type": "integer"
                  },
                  "Assignment1_delay": {
                    "type": "integer"
                  },
                  "Assignment3_submit": {
                    "type": "integer"
                  },
                  "Assignment2_submit": {
                    "type": "integer"
                  },
                  "Assignment1_submit": {
                    "type": "integer"
                  },
                  "Average_time_to_submit_assignment": {
                    "type": "integer"
                  }
                },
                "required": [
                  "features",
                  "ContentReads",
                  "No_of_Logins",
                  "Assignment2_delay",
                  "Assignment3_delay",
                  "Assignment1_delay",
                  "Assignment3_submit",
                  "Assignment2_submit",
                  "Assignment1_submit",
                  "Average_time_to_submit_assignment"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "predicted_engagement": {
                      "type": "integer"
                    },
                    "with_score": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "without_score": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "features": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "example": {
                  "error": "Model file not found, Please train the model!"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "example": {
                  "error": "No input data provided in the request body."
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "content": {
              "application/json": {
                "example": {
                  "error": "Prediction failed as data can't be processed"
                }
              }
            }
          }
        }
      }
    },
